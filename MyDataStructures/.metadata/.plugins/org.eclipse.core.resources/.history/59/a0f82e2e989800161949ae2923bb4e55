/**
 * @author: Spencer MacMaster
 * @description: contains a data value and a link to left and right TreeNode to be used in a tree data structure
 */
public class TreeNode<T> {
	public T data;
	public TreeNode<T> pi, left, right;
	
	TreeNode(T data){
		this.data = data;
		this.pi = null; this.left = null; this.right = null;
	}
}
